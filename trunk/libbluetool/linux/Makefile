include ../../Makefile.inc

#
#	Object file generation
#
CPPS = $(wildcard *.cpp ../../common/*.cpp)
OBJS = $(addsuffix .o, $(basename $(CPPS) ) )

all:		$(RULEFILE) $(OBJS)

clean:
		rm -v $(OBJS)
		rm -v $(RULEFILE)

#
#	Autogenerated rules
#
include $(RULEFILE)

rules:		$(RULEFILE)

$(RULEFILE):
		gcc $(ALLCFLAGS) -MM $(CPPS) > $(RULEFILE) 


